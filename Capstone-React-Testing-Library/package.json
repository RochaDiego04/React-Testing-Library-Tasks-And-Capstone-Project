      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "coverageReporters": [
      "html",
      "text"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx,ts,tsx}",
      "src/views/**/*.{js,jsx,ts,tsx}",
      "src/useAuth/**/*.{js,jsx,ts,tsx}",
      "src/auth/**/*.{js,jsx,ts,tsx}"
    ]
  },
  "jest-junit": {
    "outputDirectory": "coverage/junit",
    "outputName": "junit.xml"
  }
}
